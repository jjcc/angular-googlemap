{
  "extends": "./tsconfig.app.json",
  "compilerOptions": {
    "outDir": "./out-tsc/server",
    "target": "es2015",
    "module": "commonjs",
    "types": [
      "node"
    ],
    "skipLibCheck": true,
    "noImplicitAny": false,
    "strictNullChecks": false,
    "typeRoots": [
      "./node_modules/@types",
      "./src/types"
    ]
  },
  "files": [
    "src/main.server.ts",
    "server.ts"
  ],
  "angularCompilerOptions": {
    "entryModule": "./src/app/app.server.module#AppServerModule"
  }
}
